{
  "name": "learn-hunt",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "almin": "^0.13.11",
    "almin-logger": "^6.0.0",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "localstorage-ponyfill": "^1.0.1",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.5",
    "material-ui-icons": "^1.0.0-beta.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.11",
    "react-spinkit": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run assets && react-scripts build",
    "test": "mocha",
    "eject": "react-scripts eject",
    "assets": "npm run assets:images",
    "assets:images": "npm run assets:images:illustration && npm run assets:images:picture",
    "assets:images:illustration": "mkdir -p public/assets/illustration && mogrify -path public/assets/illustration -resize '1125x' -resize 'x1125>' assets/illustration/*.jpg",
    "assets:images:picture": "mkdir -p public/assets/picture && mogrify -path public/assets/picture -resize '1125x' -resize 'x1125>' assets/picture/*.jpg"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "mocha": "^3.5.0"
  }
}
